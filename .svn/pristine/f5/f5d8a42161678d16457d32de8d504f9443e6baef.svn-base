package com.chinasofti.day06.tasks;

public class Di_03_Swap3Nums {

	public static void main(String[] args) {
		int a = 12, b = 8, c = 5;

		int temp = a;
		a = b;
		b = c;
		c = temp;

		System.out.println("a = " + a + ", b = " + b + ", c = " + c);
	}

}
