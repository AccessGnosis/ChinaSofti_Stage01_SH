package com.chinasofti.day06.tasks;

public class Di_01_GetMax {

	public static void main(String[] args) {
		int a = 10, b = 8;
		
		System.out.println("max = " + getMax(a, b));
	}
	
	public static int getMax(int a, int b){
		if(a > b){
			return a;
		}else{
			return b;
		}
	}

}
